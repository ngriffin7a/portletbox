		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-processing-event>
			<qname xmlns:x="PortletSpec3_15_Events">x:AnnotatedEvent</qname>
		</supported-processing-event>
	</portlet>
	<portlet>
		<portlet-name>PORTLETSPEC3-15-Companion</portlet-name>
		<display-name>PORTLETSPEC3-15-Companion</display-name>
		<portlet-class>com.liferay.portletbox.TestPortlet3_15_Companion</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
