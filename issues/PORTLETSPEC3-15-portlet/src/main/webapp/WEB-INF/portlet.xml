		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-publishing-event>
			<qname xmlns:x="PortletSpec3_15_Events">x:AnnotatedEvent</qname>
		</supported-publishing-event>
	</portlet>
	<default-namespace>PortletSpec3_15_Events</default-namespace>
	<event-definition>
		<qname xmlns:x="PortletSpec3_15_Events">x:AnnotatedEvent</qname>
		<value-type>java.lang.String</value-type>
